{
	"entry": ["src/app/**/*.{ts,tsx}"],
	"project": ["src/**/*.{ts,tsx}"],
	"ignore": [
		"node_modules",
		"dist",
		"build",
		"public",
		"ios",
		"android",
		"web"
	],
	"ignoreDependencies": [
		"@react-navigation/bottom-tabs",
		"@react-navigation/elements",
		"@react-navigation/native",
		"expo-blur",
		"expo-crypto",
		"expo-status-bar",
		"expo-symbols",
		"jose",
		"react-native-mask-input",
		"expo-updates",
		"expo-web-browser",
		"react-dom",
		"react-native-confirmation-code-field",
		"react-native-mask-text",
		"graphql",
	],
	"ignoreUnresolved": ["babel-preset-expo"],
	"ignoreBinaries": ["graphql-inspector"]
}
